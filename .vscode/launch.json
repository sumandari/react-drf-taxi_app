  "version": "0.2.0",
  "configurations": [
    {
      "name": "Pytest: Run tests",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "justMyCode": false,
      "env": {
        "PGDATABASE": "taxi",
        "PGUSER": "taxi",
        "PGPASSWORD": "taxi"
      }
    },
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": ["runserver"],
      "django": true,
      "justMyCode": true,
      "env": {
        "PGDATABASE": "taxi",
        "PGUSER": "taxi",
        "PGPASSWORD": "taxi"
      }
    },
    {
      "name": "Django Test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": ["test"],
      "django": true,
      "justMyCode": true,
      "env": {
        "PGDATABASE": "taxi",
        "PGUSER": "taxi",
        "PGPASSWORD": "taxi"
      }
    }
  ]
}
